{
  "name": "app_scatter",
  "summary": "Generic scatter app",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "executable",
      "class": "string"
    },
    {
      "name": "batch_inputs",
      "class": "hash"
    },
    {
      "name": "common_inputs",
      "class": "hash",
      "optional":  true
    },
    {
      "name": "files",
      "class": "array:file",
      "optional":  true
    },
    {
      "name": "instance_types",
      "class": "array:string",
      "optional:": true
    }
  ],
  "outputSpec": [
    {
      "name": "results",
      "class": "hash"
    }
  ],
  "runSpec": {
    "interpreter": "python2.7",
    "file": "code.py",
    "distribution": "Ubuntu",
    "release": "14.04"
  },
  "authorizedUsers": []
}
