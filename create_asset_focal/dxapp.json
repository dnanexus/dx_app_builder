      "name": "conf_json",
      "label": "Asset configuration",
      "class": "file",
      "optional": false
    },
    {
      "name": "asset_makefile",
      "label": "A Makefile associated with the asset",
      "class": "file",
      "optional": true
    },
    {
      "name": "custom_asset",
      "label": "An archive file (tar.gz) containing custom assets",
      "class": "file",
      "optional": true
